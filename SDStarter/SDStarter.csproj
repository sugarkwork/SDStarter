<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>sds.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="controlnet.txt" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="controlnet.txt">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="sds.ico" />
  </ItemGroup>

</Project>
